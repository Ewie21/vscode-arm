{
	"name": "azalea-arm",
	"displayName": "Azalea's Arm Assembly",
	"description": "Arm assembly syntax support for Visual Studio Code, without the default comment being '@' (because my version of gcc doesn't support that)",
	"version": "1.7.4",
	"publisher": "AzaleaColburn",
	"icon": "images/icon.png",
	"author": {
		"name": "Dan Underwood and Azalea Colburn"
	},
	"categories": [
		"Programming Languages"
	],
	"engines": {
		"vscode": "^0.10.1"
	},
	"galleryBanner": {
		"color": "#0084A9",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Ewie21/vscode-arm.git"
	},
	"contributes": {
		"languages": [
			{
				"id": "arm",
				"aliases": [
					"ARM",
					"arm",
					"Arm"
				],
				"extensions": [
					".s",
					".S",
					".asm",
					".sx"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "arm",
				"scopeName": "source.arm",
				"path": "./syntaxes/arm.tmlanguage.json"
			}
		]
	}
}
